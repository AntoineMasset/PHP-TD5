/* ------ Page Liste des missions ------*/

div.filterList {
	max-width: 960px;
	width: 100%;
	background-color: #22313F;
	color: #fff;
	height: 45px;
	margin: auto;
	margin-top: 30px;
	padding-top: 20px;
	text-align: center;
}

span.filters {
	margin: 1em;
}
.search-filters {
	padding-top: 4px;
}

.missionList thead{
	max-width: 960px;
	width: 100%;
	margin: auto;
    border-collapse: separate;
    border-spacing: 10px;
    background-color: #3D3D3D;
	color: #FFF;
	position: relative;
}

table {
	width: 100%;
	max-width: 960px;
	margin: auto;
	margin-top: 20px;
	margin-bottom: 20px;
	height: 632px;
}

thead th {
	padding: 20px;
}

tbody td {
	padding: 10px;
	text-align: center;
	background-color: #ced2d7;
	border-width: 1px;
	border-style: ridge;
}

table img {
	width: 30px;
	height: 30px;
}

.nav-table {
	width: 100%;
	padding: 5px;
	height: 20px;
	max-width: 960px;
	margin: auto;
	margin-bottom: 20px;
	text-align: center;
}

.nav-table ul li {
	display: inline;
	margin: 5px;
	font-weight: bold;
}

.nav-table ul li a {
	color: #000;
}

.nav-table ul li a:hover {
	text-decoration: underline;
}

.nav-table ul li button {
	background-color:#3d3d3d;
	cursor:pointer;
	color:#ffffff;
	font-size:15px;
	font-weight:bold;
	border-width: 0px;
	padding:9px 23px;
	text-decoration:none;
}
.myButton:hover {
	background-color:#3d3d3d;
}
.myButton:active {
	position:relative;
	top:1px;
}

li.table-select-page {
	color: #bdc3c7;
}


div.newMission-button {
	text-align: center;
	margin: auto;
	margin-bottom: 10px;
}

div.newMission-button button {
	background-color:#3d3d3d;
	cursor:pointer;
	color:#ffffff;
	font-size:15px;
	font-weight:bold;
	border-width: 0px;
	padding:9px 23px;
	text-decoration:none;
}

div.newMission-form {
	background-color: #3D3D3D;
	padding: 20px;
	width: 50%;
	margin: auto;
	margin-bottom: 20px;
	color: #fff;
}

.newMission-form form {
	display: table;
	margin: auto;
}

div.newMission-form-element {
	display: table-row;
	margin: auto;
}

div.newMission-form-element input, div.newMission-form-element select, div.newMission-form-element label{
	display: table-cell;
	margin: 10px;
}

div.newMission-form-element input, div.newMission-form-element select {
	width: 15em;
}

div.intervalDate input {
	width: auto;
}

input#newMission {
	background: #FFFFFF url(../images/arrow.png) no-repeat;
    background-position: right;
    background-size: 25px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    -moz-box-shadow: 0 0 1px rgba(0, 0, 0, 0.3), 0 1px 0 rgba(255, 255, 255, 0.3) inset;
    -webkit-box-shadow: 0 0 1px rgba(0, 0, 0, 0.3), 0 1px 0 rgba(255, 255, 255, 0.3) inset;
    box-shadow: 0 0 1px rgba(0, 0, 0, 0.3), 0 1px 0 rgba(255, 255, 255, 0.3) inset;    
    border-width: 1px;
    border-style: solid;
    border-color: #3f3f3f;
    height: 35px;
    width: 120px;
    cursor: pointer;
    font: bold 15px Arial, Helvetica;
    color: #3f3f3f;
    margin-top: 20px;
    margin-bottom: 10px;
}

.arrow-up-mission {
	width: 0px;
	margin: auto;
	border-left: 15px solid transparent;
	border-right: 15px solid transparent;
	border-bottom: 15px solid #3D3D3D;
}

