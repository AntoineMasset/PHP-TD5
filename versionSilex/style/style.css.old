/* PAGE CONNEXION */

#login
{
	clear: both;
	color: white;
	margin: auto;
    background-color: #818181;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#818181), to(#606060));
    background-image: -webkit-linear-gradient(top, #818181, #606060);
    background-image: -moz-linear-gradient(top, #818181, #606060);
    background-image: -ms-linear-gradient(top, #818181, #606060);
    background-image: -o-linear-gradient(top, #818181, #606060);
    background-image: linear-gradient(top, #818181, #606060);  
    height: 200px;
    width: 400px;
    padding: 30px;
}

#login a {
	color: white;
	float: right;	
	font-size: 0.8em;
}

fieldset
{
    border: 0;
    padding: 0;
    margin: 0;
}

#conn-infos input
{
    background: #f1f1f1 url(../images/login-sprite.png) no-repeat;
    padding: 15px 15px 15px 30px;
    margin: 0 0 10px 0;
    width: 353px; /* 353 + 2 + 45 = 400 */
    border: 1px solid #ccc;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px;
    -moz-box-shadow: 0 1px 1px #ccc inset, 0 1px 0 #fff;
    -webkit-box-shadow: 0 1px 1px #ccc inset, 0 1px 0 #fff;
    box-shadow: 0 1px 1px #ccc inset, 0 1px 0 #fff;
}

#username
{
    background-position: 5px -2px !important;
}

#password
{
    background-position: 5px -52px !important;
}

#conn-infos input:focus
{ /* couleur entourant l'input lors d'un clic */
    background-color: #fff;
    border-color: #606060;
    outline: none;
    -moz-box-shadow: 0 0 0 1px #606060 inset;
    -webkit-box-shadow: 0 0 0 1px #606060 inset;
    box-shadow: 0 0 0 1px #606060 inset;
}

#actions
{
    margin: 25px 0 0 0;
}

#submit
{		
    background: #FFFFFF url(../images/arrow.png) no-repeat;
    background-position: right;
    background-size: 25px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    -moz-box-shadow: 0 0 1px rgba(0, 0, 0, 0.3), 0 1px 0 rgba(255, 255, 255, 0.3) inset;
    -webkit-box-shadow: 0 0 1px rgba(0, 0, 0, 0.3), 0 1px 0 rgba(255, 255, 255, 0.3) inset;
    box-shadow: 0 0 1px rgba(0, 0, 0, 0.3), 0 1px 0 rgba(255, 255, 255, 0.3) inset;    
    border-width: 1px;
    border-style: solid;
    border-color: #3f3f3f;
    float: right;
    height: 35px;
    padding-right: 20px;
    width: 120px;
    cursor: pointer;
    font: bold 15px Arial, Helvetica;
    color: #3f3f3f;
}

#submit:hover,#submit:focus
{		
	background: #bdc3c7 url(../images/arrow.png) no-repeat;
    background-position: right;
    background-size: 25px;
}	

#actions a, #rememberme, #actions label
{  
	padding-top: 5px;
    float: left;
}

