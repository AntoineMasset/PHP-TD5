body{
	font-family: 'Roboto', sans-serif;
	padding-bottom: 0px;
}

a {
	text-decoration: none;
}
img
{
	width: 100%;
}
#wrapper {
	width: 100%;
	max-width: 1024px;
	margin: auto;
	overflow: hidden;
}

.header {
	width: 100%;
	margin: auto;
	overflow: hidden;
}	

.header a {
	color: #000000;
}

.head {
	float: left;
	width: 900px;
	margin: 0 0 10px 0;
}

.logo>a>img {
	width: 170px;
	float: left;
	margin: 10px 0 0 40px;
}

.title {
	font-family: 'Oswald', sans-serif;
	float: left;
	margin: 40px 0 0 30px;
}

.title>h1 {
	font-size: 2.0em;
	margin: 5px 0 5px 65px;
}

.title h2 {
	font-size: 1.3em;
}

.title h2:last-child {
	margin-left: 300px;
}

.header-btns {
	float: right;
	margin: 45px 50px 0 0;
}

.header-btns>a {
	background: #22313F;
	color: white;
	padding: 8px;
}

.flags {
	margin: 0 0 30px 14px;
}

.flags a img {
	width: 25px;
}

 .flags>a>img {
 	margin: 0 7px 0 0;
 }
 
.nav {
	text-transform: uppercase;
	font-weight:bold;
	clear: both;
	text-align:center;
}
.nav ul {
	padding: 0; margin: 0;
	text-align: center;
}

.nav ul li {
	display: inline;
	list-style: none;
	margin: 0 40px 0 40px;
}

.nav ul li a, .nav ul li form {
	padding: 15px 15px 10px 15px;
	display: inline-block;
	border-bottom:0px solid #75818B;
    -webkit-transition: border .5s ease-in;
    -moz-transition: border .5s ease-in;
    -o-transition: all .5s ease-in;
    transition: border .5s ease-in;
}

.nav ul li a:hover {
	border-bottom: 5px solid #75818B;
	color: #75818B;
}

.nav ul li {
	display:inline;
	list-style:none;
	margin: 0 40px 0 40px;
}

.nav ul li a, .nav ul li form{
	padding: 15px 18px 15px 18px;
	display: inline-block;
}

.nav ul li:last-child {
	margin-top: -5px;
}

